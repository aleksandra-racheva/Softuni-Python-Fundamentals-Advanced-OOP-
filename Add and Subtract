def sum_numbers(int1,int2):
    sum = int1 + int2
    return sum


def subtract(sum,int2):
    result = sum - int2
    return result

    
integer1 = int(input())
integer2 = int(input())
integer3 = int(input())

def add_and_subtract(int1,int2,int3):
    sum2 = sum_numbers(integer1,integer2)
    final_subtract = subtract(sum2,integer3)
    return final_subtract

print(add_and_subtract(integer1,integer2,integer3))
