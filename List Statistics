num = int(input())

positive = list()
negative = list()
num_positives = 0
sum_of_negatives = 0

for _ in range(num):
    nums = int(input())
    if nums < 0:
        sum_of_negatives += nums
        negative.append(nums)
    else:
        num_positives += 1
        positive.append(nums)

print(positive)
print(negative)
print(f"Count of positives: {num_positives}")
print(f"Sum of negatives: {sum_of_negatives}")
