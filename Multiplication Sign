def output(num1,num2,num3):
    new_list = []
    new_list.append(num1)
    new_list.append(num2)
    new_list.append(num3)
    
    multiplication = new_list[0] * new_list[1] * new_list[2]
    
    if multiplication == 0:
        return "zero"
    elif multiplication > 0:
        return "positive"
    else:
        return "negative"
    
    
number1 = int(input())
number2 = int(input())
number3 = int(input())

print(output(number1,number2,number3))
